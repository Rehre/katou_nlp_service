{
  "name": "katou_nlp_service",
  "version": "1.0.0",
  "description": "Katou system nlp service api",
  "main": "index.js",
  "scripts": {
    "start": "rm -rf build && npm run build && cross-env NODE_ENV=production node build/index.js",
    "build": "./node_modules/.bin/babel src -d lib",
    "dev": "cross-env NODE_ENV=express nodemon --watch src --exec babel-node src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint src --fix",
      "git add ."
    ]
  },
  "keywords": [
    "Katou",
    "service"
  ],
  "author": "Akmal Maulana Azzam",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "brain.js": "^1.6.1",
    "express": "^4.16.4",
    "natural": "^0.6.3"
  }
}
